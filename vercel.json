{
  "buildCommand": "echo 'Ready for deployment'",
  "framework": "static",
  "cleanUrls": true,
  "trailingSlash": false,
  "env": [
    "CORS_ORIGIN",
    "NODE_ENV",
    "VERCEL_URL"
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
