{
  "buildCommand": "echo 'Ready for deployment'",
  "framework": "express",
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {},
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "\/(.*)",
      "destination": "/index.html"
    }
  ]
}
